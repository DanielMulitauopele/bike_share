<h1>All Admin Stations</h1>

<ul id="stations">
  <% @stations.each do |station| %>
    <li>
      Station: <%= link_to station.name, station_path(station), class: 'stations-name' %>
      Dock Count: <%= station.dock_count %>
      City: <%= station.city %>
      Installed: <%= station.installation_date %>
      <%= link_to 'Edit' %>
      <%= link_to 'Delete' %>
    </li>
  <% end %>
</ul>
