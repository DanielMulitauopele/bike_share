

<div class="container">
  <div class="row">
    <div class="col-sm"></div>
    <div class="col-sm">

      <%= form_for [:admin, @station] do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      </br>

      <%= f.label :dock_count %>
      <%= f.text_field :dock_count %>
      </br>

      <%= f.label :city %>
      <%= f.text_field :city %>
      </br>

      <%= f.label :installation_date %>
      <%= f.date_field :installation_date %>
      </br>

      <%= f.submit 'Submit' %>
      <% end %>

    </div>
    <div class="col-sm"></div>
  </div>
</div>
