
<%= form_for [:admin, @accessory] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :status %>
  <%= f.select :status, options_for_select(["Ordered", "Completed", "Paid", "Canceled"]) %>
  <%= f.submit %>
<% end %>
