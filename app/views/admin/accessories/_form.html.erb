
<%= form_for [:admin, @accessory] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.label :status %>
  <%= f.select :status, options_for_select(["active", "retired"]) %>
  <%= f.submit %>
<% end %>
